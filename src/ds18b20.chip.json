{
    "name": "Dallas DS18B20",
    "author": "Bonny Rais",
    "docs": "https://github.com/bonnyr/wokwi-ds1820-custom-chip/blob/main/README.md",
    "pins": [
      "DQ",
      "GND",
      "VCC"
    ],
    "controls": [
      {
        "id": "temperature",
        "label": "DS18B20 Temperature",
        "type": "range",
        "min": -55,
        "max": 125,
        "step": 0.125
      },
      {
        "id": "owDebug",
        "label": "OneWire Debug control",
        "type": "boolean",
        "min": 0,
        "max": 1,
        "step": 1
      },
      {
        "id": "deviceID",
        "label": "device serial number",
        "type": "string"
      }
    ]
  }